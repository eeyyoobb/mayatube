{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/(routes)/quizzes/quiz-start/page.tsx",
    "src/app/(routes)/quizzes/page.jsx",
    "src/app/(routes)/quizzes/page.jsx",
    "src/app/api/quizzes/route.js",
    "src/components/quiz/QuizzesAa.js",
    "src/context/QuizProvider.js",
    "src/components/quiz/QuizCard.js",
    "src/components/quiz/QuizzesArea.js",
    "src/app/(routes)/quizzes/layout.jsx",
    "src/components/quiz/QuizCustom.js",
    "src/app/(routes)/quizzes/custom/page.js",
    "src/app/(routes)/quizzes/custom/page.jsx",
    "src/context/MarketProvider.js",
    "src/app/(routes)/quizzes/custom/quiz/page.tsx",
    "src/components/quiz/custom/Hero.jsx",
    "src/lib/validators/*.ts",
    "src/server.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
