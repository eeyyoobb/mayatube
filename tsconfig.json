{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/(routes)/quizzes/quiz-start/page.tsx", "src/app/(routes)/quizzes/page.jsx", "src/app/(routes)/quizzes/page.jsx", "src/app/api/quizzes/route.js", "src/components/quiz/QuizzesAa.js", "src/context/QuizProvider.js", "src/components/quiz/QuizCard.js", "src/components/quiz/QuizzesArea.js", "src/app/(routes)/quizzes/layout.jsx"],
  "exclude": ["node_modules"]
}
